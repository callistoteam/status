        "module": "commonjs",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "outDir": "dist",
        "lib": [ "es6" ]
    },
    "exclude": [
        "node_modules"
    ]
}